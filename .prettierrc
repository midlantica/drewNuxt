{
  "singleQuote": true,
  "semi": true,
  "singleAttributePerLine": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "htmlWhitespaceSensitivity": "ignore",
  "bracketSameLine": false,
  "trailingComma": "none",
  "jsxBracketSameLine": false,
  "endOfLine": "auto",
  "htmlVoidTags": false,
  "arrowParens": "avoid",
  "vueIndentScriptAndStyle": true,
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "singleAttributePerLine": false
      }
    }
  ]
}
